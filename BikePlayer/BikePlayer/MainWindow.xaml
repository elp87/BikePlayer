<Window x:Class="BikePlayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="PlayButton" Content="Play" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,47,0,0" Click="PlayButton_Click"/>
        <Button Content="Pause" HorizontalAlignment="Left" Margin="92,47,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Label x:Name="ContentLabel" Content="" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Slider x:Name="TimeSlider" Margin="172,49,10,0" VerticalAlignment="Top" PreviewMouseLeftButtonUp="TimeSlider_PreviewMouseLeftButtonUp"  />


    </Grid>
</Window>
